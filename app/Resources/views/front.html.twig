    <!-- END HEADER -->
{% endblock header %}

{%  block page_container %}
<div class="page-wrappers m-t-80 {% block extra_class_page_wrapper ''%}">

    {% block jumbotron %}
    {% endblock jumbotron %}

    <!-- BEGIN PlACE PAGE CONTENT HERE -->
    {% block page_content %}
    {% endblock page_content %}
    <!-- END PLACE PAGE CONTENT HERE -->

    {% block footer %}
        {% include "::frontFooter.html.twig" %}
    {% endblock footer %}

</div>
{%  endblock page_container %}

    {% include "WelcomangoCoreBundle:Core:loginModal.html.twig" %}

<!-- BEGIN VENDOR JS -->
{% block javascripts %}
    <script type="text/javascript" src="{{ asset("js/plugins-front.js") }}"></script>
<!-- END VENDOR JS -->
<!-- BEGIN PAGES FRONTEND LIB -->
    <script type="text/javascript" src="{{ asset("js/pages-front.js") }}"></script>
    <script src="{{ asset('js/plugins.js') }}" type="text/javascript"></script>
    <script>
        (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
            (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
                m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
        })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

        ga('create', 'UA-73728528-1', 'auto');
        ga('send', 'pageview');

    </script>

    <script>
        window.fbAsyncInit = function() {
            FB.init({
                appId      : '1109739712422558',
                xfbml      : true,
                version    : 'v2.5'
            });
        };

        (function(d, s, id){
            var js, fjs = d.getElementsByTagName(s)[0];
            if (d.getElementById(id)) {return;}
            js = d.createElement(s); js.id = id;
            js.src = "//connect.facebook.net/en_US/sdk.js";
            fjs.parentNode.insertBefore(js, fjs);
        }(document, 'script', 'facebook-jssdk'));
    </script>
    <!-- END PAGES LIB -->
{% endblock %}
{% endblock inner_body %}
</body>
{% endblock body %}
</html>
